<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Point">

	<update id="updatePointByUserId" parameterType="zh.co.item.bank.model.entity.UserModel">
		update tu_point_history set point = #{point} where user_id = #{id}
	</update>

</mapper>