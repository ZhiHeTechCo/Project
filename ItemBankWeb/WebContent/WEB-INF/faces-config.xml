<?xml version="1.0" encoding="UTF-8"?>

<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

<application>
    <el-resolver>
        org.springframework.web.jsf.el.SpringBeanFacesELResolver
    </el-resolver>
</application>

<factory>
  <external-context-factory>zh.co.common.factory.ExternalContextFactory</external-context-factory>
</factory> 
<navigation-rule>
   <from-view-id>*</from-view-id>
   <navigation-case>
      <from-outcome>ITBK_COM_001</from-outcome>
      <to-view-id>/xhtml/common/home.xhtml</to-view-id>
   </navigation-case>
   <navigation-case>
      <from-outcome>ITBK_COM_002</from-outcome>
      <to-view-id>/xhtml/common/aboutUs.xhtml</to-view-id>
   </navigation-case>
   <navigation-case>
      <from-outcome>ITBK_USER_001</from-outcome>
      <to-view-id>/xhtml/user/signUp.xhtml</to-view-id>
   </navigation-case>
   <navigation-case>
      <from-outcome>ITBK_USER_002</from-outcome>
      <to-view-id>/xhtml/user/signIn.xhtml</to-view-id>
   </navigation-case>
   <navigation-case>
      <from-outcome>ITBK_USER_003</from-outcome>
      <to-view-id>/xhtml/user/userInfo.xhtml</to-view-id>
   </navigation-case>
   <navigation-case>
      <from-outcome>ITBK_USER_004</from-outcome>
      <to-view-id>/xhtml/user/changePassword.xhtml</to-view-id>
   </navigation-case>
   <navigation-case>
      <from-outcome>ITBK_USER_005</from-outcome>
      <to-view-id>/xhtml/user/mistakeCollection.xhtml</to-view-id>
   </navigation-case>
   
   
   <navigation-case>
      <from-outcome>page_forward_error</from-outcome>
      <to-view-id>/xhtml/common/page_forward_error.xhtml</to-view-id>
      <redirect/>
   </navigation-case>
</navigation-rule>

</faces-config>